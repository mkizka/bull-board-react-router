{
  "name": "bull-board-react-router",
  "version": "1.0.1",
  "type": "module",
  "description": "A bull-board adapter for React Router v7",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkizka/bull-board-react-router.git"
  },
  "author": "mkizka",
  "license": "MIT",
  "packageManager": "pnpm@10.29.2",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write . && eslint --fix .",
    "prepare": "husky",
    "release": "np --no-tests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bull-board/api": "6.16.4",
    "ejs": "3.1.10",
    "mime": "^4.1.0",
    "urlpattern-polyfill": "10.1.0"
  },
  "devDependencies": {
    "@mkizka/eslint-config": "^8.0.0",
    "@tsconfig/node-lts": "24.0.0",
    "@tsconfig/strictest": "2.0.8",
    "@types/ejs": "3.1.5",
    "@types/node": "^25.2.3",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "np": "11.0.2",
    "prettier": "^3.8.1",
    "prettier-plugin-pkg": "^0.21.2"
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write",
      "eslint --fix"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-pkg"
    ]
  }
}
